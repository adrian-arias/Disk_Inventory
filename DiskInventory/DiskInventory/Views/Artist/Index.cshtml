@model List<Artist>
@{
    ViewData["Title"] = "Artist Page";
}
<h2>Artist List</h2>

<table class="table table-bordered table-striped">
    <thead>
        <tr><th>ArtistId</th><th>First Name</th><th>Last Name</th><th>Artist Type</th></tr>
    </thead>
    <tbody>
        @foreach (var artist in Model)
        {
            <tr>
                <td>@artist.ArtistId</td>
                <td>@artist.Fname</td>
                <td>@artist.Lname</td>
                <td>@artist.ArtistTypeId</td>
            </tr>
        }
    </tbody>
</table>